# Production Environment Variables
# Copy this file to .env.production and fill in the actual values

# GitHub Container Registry
GITHUB_REPOSITORY=your-username/storage-manager
IMAGE_TAG=latest

# MongoDB Atlas Connection
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/storage-prod?retryWrites=true&w=majority

# Client URL (Frontend URL)
CLIENT_URL=https://storage-app.example.com

# Optional: Docker Compose Project Name
COMPOSE_PROJECT_NAME=storage-manager-prod
